{
  "extends": ["@uifabric/tslint-rules"],
  "rulesDirectory": "lintCustom",
  "rules": {
    "deprecation": false,
    "jsx-ban-props": false,
    "no-any": false,
    "typedef": [false],
    "custom-import-blacklist": [
      true,
      {
        "modulesWithBannedImports": [[".*styling.*", "^FontSizes$", "DefaultFontStyles"]]
      }
    ]
  }
}
