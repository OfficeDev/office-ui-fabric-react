{
  "extends": [
    "office-ui-fabric-react-tslint"
  ],
  "rules": {
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "no-any": false,
    "no-null-keyword": false,
    "variable-name": false,
    "export-name": false,
    "trailing-comma": [
      false
    ],
    "no-internal-module": false,
    "prefer-const": false
  }
}